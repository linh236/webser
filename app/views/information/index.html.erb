<p id="notice"><%= notice %></p>

<h1>Information</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sex</th>
      <th>Birth</th>
      <th>Indentifycard</th>
      <th>Daterange</th>
      <th>Placerange</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Permanent</th>
      <th>Start</th>
      <th>Deposit</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @information.each do |information| %>
      <tr>
        <td><%= information.name %></td>
        <td><%= information.sex %></td>
        <td><%= information.birth %></td>
        <td><%= information.indentifycard %></td>
        <td><%= information.daterange %></td>
        <td><%= information.placerange %></td>
        <td><%= information.phone1 %></td>
        <td><%= information.phone2 %></td>
        <td><%= information.permanent %></td>
        <td><%= information.start %></td>
        <td><%= information.deposit %></td>
        <td><%= information.note %></td>
        <td><%= link_to 'Show', information %></td>
        <td><%= link_to 'Edit', edit_information_path(information) %></td>
        <td><%= link_to 'Destroy', information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Information', new_information_path %>
