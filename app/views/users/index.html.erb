<table class="table table-hover service">
  <thead>
    <tr>
      <th class="text-center">STT</th>
      <th class="text-center">Nhà/Phòng</th>
      <th class="text-center">Họ và tên</th>
      <th class="text-center">Giới tính</th>
      <th class="text-center">CMND</th>
      <th class="text-center">Điện thoại 1</th>
      <th class="text-center">Điện thoại 2</th>
      <th class="text-center">Email</th>
      <th class="text-center">Bắt đầu ở</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do |user, i|%>
    <tr class="info_cus" data-id="<%= user.id%>">
      <td class="text-center"><%=i + 1%></td>
      <td class="text-center"><%= getHouseRoom(user.id)%></td>
      <td class="text-center"><%= user.name%></td>
      <td class="text-center"><%= user.sex == false ? "Nam" : "Nữ"%></td>
      <td class="text-center"><%= user.indentifycard%></td>
      <td class="text-center"><%= user.phone1%></td>
      <td class="text-center"><%= user.phone2%></td>
      <td class="text-center"><%= user.email%></td>
      <td class="text-center"><%= user.start%></td>
    </tr>
    <% end %>
  </tbody>
</table>
